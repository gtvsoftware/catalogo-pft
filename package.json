{
  "name": "@terraviva/front-repo",
  "private": true,
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d",
  "scripts": {
    "build:all": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "dev:all": "turbo dev --parallel",
    "dev": "turbo dev --filter=@terraviva/catalogo-pft",
    "build": "turbo build --filter=@terraviva/catalogo-pft",
    "lint": "biome lint apps/",
    "lint:fix": "biome lint --write apps/",
    "format": "biome format apps/",
    "format:fix": "biome format --write apps/",
    "check": "biome check apps/",
    "check:fix": "biome check --write apps/",
    "typecheck": "turbo typecheck",
    "test": "exit 0",
    "prepare": "if [ -z \"$CI\" ] && [ -z \"$DOCKER_BUILDKIT\" ]; then husky install; fi",
    "db:seed": "cd packages/db-catalogo-pft && pnpm prisma generate && pnpm run db:seed && cd ../..",
    "typesense:seed": "cd packages/typesense-catalogo-pft && pnpm run typesense:seed && cd ../.."
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@terraviva/tsconfig": "^0.0.1",
    "husky": "^5",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.6",
    "turbo": "2.5.8",
    "typescript": "5.9.3"
  }
}
