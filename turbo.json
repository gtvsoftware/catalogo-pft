{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "ui": "stream",
  "globalEnv": [],
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "env": [
        "NODE_ENV",
        "AUTH_URL",
        "AUTH_SECRET",
        "AUTH_ISSUER_URL",
        "AUTH_CLIENT_ID",
        "AUTH_CLIENT_SECRET",
        "DATABASE_URL"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        "generated/**"
      ]
    },
    "start": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "dev": {
      "env": [
        "NODE_ENV",
        "AUTH_URL",
        "AUTH_SECRET",
        "AUTH_ISSUER_URL",
        "AUTH_CLIENT_ID",
        "AUTH_CLIENT_SECRET",
        "DATABASE_URL"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "persistent": true,
      "cache": false
    },
    "jobs": {
      "persistent": true,
      "cache": false
    },
    "format": {},
    "lint": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    }
  }
}